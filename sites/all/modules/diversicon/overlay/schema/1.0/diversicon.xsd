    <xs:complexType/>
  </xs:element>
  <!-- #################### Meta Classes -->
  <xs:element name="Frequency">
    <xs:complexType>
      <xs:attribute name="corpus"/>
      <xs:attribute name="frequency"/>
      <xs:attribute name="generator"/>
    </xs:complexType>
  </xs:element>
  <!--
    corpus		http://www.isocat.org/datcat/DC-4361
    frequency 	http://www.isocat.org/datcat/DC-238
    generator	http://www.isocat.org/datcat/DC-2511
  -->
  <xs:element name="MetaData">
    <xs:complexType>
      <xs:attribute name="id" use="required" type="xs:ID"/>
      <xs:attribute name="creationDate" use="required"/>
      <xs:attribute name="creationTool" use="required"/>
      <xs:attribute name="version" use="required"/>
      <xs:attribute name="creationProcess" use="required"/>
      <xs:attribute name="automatic" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SemanticLabel">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="MonolingualExternalRef"/>
      </xs:sequence>
      <xs:attribute name="label"/>
      <xs:attribute name="type"/>
      <xs:attribute name="quantification"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
