    externalReference	http://www.isocat.org/datcat/DC-164
  -->
  <xs:element name="TextRepresentation">
    <xs:complexType>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="languageIdentifier"/>
      <xs:attribute name="orthographyName"/>
      <xs:attribute name="geographicalVariant"/>
      <xs:attribute name="writtenText" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--
    languageIdentifier 	http://www.isocat.org/datcat/DC-279
    orthographyName 	http://www.isocat.org/datcat/DC-2176
