        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="verbFormMood">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="infinitive"/>
            <xs:enumeration value="infinitiveZu"/>
            <xs:enumeration value="participle"/>
            <xs:enumeration value="indicative"/>
            <xs:enumeration value="subjunctive"/>
            <xs:enumeration value="imperative"/>
            <xs:enumeration value="ingForm"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="degree">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="positive"/>
            <xs:enumeration value="comparative"/>
            <xs:enumeration value="superlative"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
