          <xs:restriction base="xs:token">
            <xs:enumeration value="etymology"/>
            <xs:enumeration value="compound"/>
            <xs:enumeration value="compoundRoot"/>
            <xs:enumeration value="derivative"/>
            <xs:enumeration value="derivationBase"/>
            <xs:enumeration value="derivationBaseNoun"/>
            <xs:enumeration value="derivationBaseVerb"/>
            <xs:enumeration value="derivationBaseAdj"/>
            <xs:enumeration value="derivationBaseVerbAdj"/>
            <xs:enumeration value="loanWord"/>
            <xs:enumeration value="supports"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!--
    etymology http://www.isocat.org/datcat/DC-221
