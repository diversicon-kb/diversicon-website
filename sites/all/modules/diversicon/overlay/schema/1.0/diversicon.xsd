  -->
  <xs:element name="Statement">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TextRepresentation"/>
      </xs:sequence>
      <xs:attribute name="definition"/>
      <xs:attribute name="statementType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="usageNote"/>
            <xs:enumeration value="encyclopedicInformation"/>
            <xs:enumeration value="etymology"/>
            <xs:enumeration value="backgroundText"/>
            <xs:enumeration value="externalReference"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Statement	http://www.isocat.org/datcat/DC-382 -->
