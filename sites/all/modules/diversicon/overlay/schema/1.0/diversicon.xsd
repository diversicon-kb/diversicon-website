    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="FormRepresentation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Frequency"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:ID"/>
      <xs:attribute name="grammaticalNumber">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="singular"/>
            <xs:enumeration value="plural"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="grammaticalGender">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="feminine"/>
            <xs:enumeration value="masculine"/>
            <xs:enumeration value="neuter"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="case">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="nominative"/>
            <xs:enumeration value="genitive"/>
            <xs:enumeration value="dative"/>
            <xs:enumeration value="accusative"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="person">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="first"/>
            <xs:enumeration value="second"/>
            <xs:enumeration value="third"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="tense">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="present"/>
            <xs:enumeration value="past"/>
